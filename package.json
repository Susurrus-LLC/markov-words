{
  "name": "markov-words",
  "version": "1.0.0",
  "description": "Word generator based on Markov chains",
  "homepage": "https://susurrus-llc.github.io/markov-words/",
  "repository": {
    "type": "git",
    "url": "https://github.com/Susurrus-LLC/markov-words"
  },
  "bugs": {
    "url": "https://github.com/Susurrus-LLC/markov-words/issues"
  },
  "author": "Ian A. Cook <ian@code.ianacook.com>",
  "license": "AGPL-3.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build",
    "format": "prettier-standard src/**/*.{js,jsx,ts,tsx}",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier-standard"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.13.1",
    "@types/jest": "26.0.4",
    "@types/node": "14.0.14",
    "@types/react": "16.9.41",
    "@types/react-dom": "16.9.8",
    "node-sass": "^4.14.1",
    "normalize.css": "^8.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "typescript": "3.9.6"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^3.1.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "modularscale-sass": "^3.0.10",
    "prettier-standard": "^16.3.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "standard-with-typescript"
    ],
    "parserOptions": {
      "project": "./tsconfig.json"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
